    "string='ABABA'\n",
    "pat='ABA'\n",
    "\n",
    "m=len(string)\n",
    "n=len(pat)\n",
    "for i in range(m-n+1):\n",
    "    j=0\n",
    "    while j<n: \n",
    "        if string[i+j]!=pat[j]:\n",
    "            break\n",
    "        j+=1\n",
    "    if j==n:\n",
    "        print(\"Pattern Found At Index\",i)"
